<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jvyuan" />

    <TextView
        android:id="@+id/ding_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="痛风"
        android:textColor="#F8F9F5"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView37"
        app:layout_constraintEnd_toEndOf="@+id/imageView37"
        app:layout_constraintStart_toStartOf="@+id/imageView37"
        app:layout_constraintTop_toTopOf="@+id/imageView37" />

    <TextView
        android:id="@+id/ding_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="￥500.00/月"
        android:textColor=" #333333 "
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/ding_name"
        app:layout_constraintStart_toEndOf="@+id/imageView37"
        app:layout_constraintTop_toTopOf="@+id/ding_name" />

    <CheckBox
        android:id="@+id/cb_customized_service_item"
        android:layout_width="20dp"
        android:layout_height="@dimen/dp_20"
        android:layout_marginEnd="15dp"
        android:background="@drawable/ding_xuan"
        android:button="@null"
        app:layout_constraintBottom_toBottomOf="@+id/ding_money"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ding_money" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="15dp"
        android:background="#CCCCCC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView37"></View>
</androidx.constraintlayout.widget.ConstraintLayout>
